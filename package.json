{
  "name": "picky-demo",
  "version": "1.0.0",
  "description": "color-picky demo page",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-js & npm run build-css",
    "build-js": "browserify ./index.js -r color-picky -r queried -r color | ccjs - --language_in=ECMASCRIPT5 > bundle.js",
    "build-css": "postcss -u postcss-import -u autoprefixer -u cssnano index.css -o bundle.css",
    "watch": "watchify ./index.js -r color-picky -r queried -r color -d -o bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dfcreative/picky.git"
  },
  "author": "Deema Ywanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfcreative/picky/issues"
  },
  "homepage": "https://github.com/dfcreative/picky",
  "dependencies": {
    "color": "^0.8.0",
    "color-picky": ">0.0.2",
    "normalize.css": "^3.0.3",
    "picky": "^0.1.0",
    "queried": "^1.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "browserify": "^10.2.3",
    "closurecompiler": "^1.5.2",
    "cssnano": "^1.3.0",
    "postcss": "^4.1.11",
    "postcss-cli": "^1.3.1",
    "postcss-import": "^5.2.2"
  }
}
